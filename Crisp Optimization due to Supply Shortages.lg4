\cf1 !OPTIMIZATION MODEL;\cf2\par
\par
\cf1 !Optimization Objective;\cf2\par
\cf3 MAX\cf2  = REVENUE;\par
REVENUE = \cf3 @SUM\cf2 (OUTPUTSTREAM(L):PRICE(L)*YSTAR(L));\par
\par
\cf1 !Mass Balances;\cf2\par
\cf3 @FOR\cf2 (OUTPUTSTREAM(L): YSTAR(L) = \cf3 @SUM\cf2 (PROCESS(K):A(L,K)*XSTAR(K)));\par
\cf3 @FOR\cf2 (INPUTSTREAM(J): ZINPUT(J) = \cf3 @SUM\cf2 (PROCESS(K):B(J,K)*XSTAR(K)));\par
\par
\cf1 !Input Substitution;\cf2\par
\cf3 @FOR\cf2 (RAWPLASTICWASTE(I):\cf3 @FOR\cf2 (INPUTSTREAM(J): Z(I,J) < S(I,J)*M));\par
\cf3 @FOR\cf2 (INPUTSTREAM(J): ZINPUT(J) = \cf3 @SUM\cf2 (RAWPLASTICWASTE(I):Z(I,J)));\par
\cf3 @FOR\cf2 (RAWPLASTICWASTE(I): ZRAW(I) > \cf3 @SUM\cf2 (INPUTSTREAM(J):Z(I,J)));\par
\par
\cf1 !Limit of Substitution;\cf2\par
\cf3 @FOR\cf2 (RAWPLASTICWASTE(I):\cf3 @FOR\cf2 (INPUTSTREAM(J)|I#EQ#J: Z(J,J) > (1 - SMAX(J))*ZINPUT(J)));\par
\par
\cf1 !Operating Ranges of each Component Unit;\cf2\par
\cf3 @FOR\cf2 (PROCESS(K): XMINPER(K)*XMAX(K)*T(K) < XSTAR(K));\par
\cf3 @FOR\cf2 (PROCESS(K): XSTAR(K) < XMAX(K)*T(K));\par
\cf3 @FOR\cf2 (PROCESS(K): \cf3 @BIN\cf2 (T(K)));\par
\par
